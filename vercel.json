{
    "version": 2,
    "builds": [
      {
        "src": "asy/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", 
          "runtime": "python3.10"
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "asy/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "asy/wsgi.py"
      }
    ],
    "buildCommand": "npm install && python manage.py collectstatic --noinput && cp -r staticfiles/* public/",
    "env": {
      "PYTHONUNBUFFERED": "1",
      "DJANGO_SETTINGS_MODULE": "asy.settings"
    }
  }
  