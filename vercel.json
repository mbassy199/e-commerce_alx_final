{
    "version": 2,
    "builds": [
        {
            "src": "asy/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb", 
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "asy/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "asy/wsgi.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "DJANGO_SETTINGS_MODULE": "asy.settings"
    }

}